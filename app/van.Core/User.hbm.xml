<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true" assembly="van.Core" namespace="van.Core">
  <class name="User" table="Users" xmlns="urn:nhibernate-mapping-2.2">
    <id name="Id" column="Id" type="Int32">
      <generator class="identity" />
    </id>
    <property name="LastName" length="100" type="String">
      <column name="LastName" />
    </property>
    <property name="UserName" length="100" type="String">
      <column name="UserName" />
    </property>
    <property name="FirstName" length="100" type="String">
      <column name="FirstName" />
    </property>
    <property name="Email" length="100" type="String">
      <column name="Email" />
    </property>
    <property name="Password" length="100" type="String">
      <column name="Password" />
    </property>
  </class>
</hibernate-mapping>