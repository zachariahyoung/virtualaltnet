<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true" assembly="van.Core" namespace="van.Core">
  <class name="Recording" table="Recordings" xmlns="urn:nhibernate-mapping-2.2">
    <id name="Id" column="Id" type="Int32">
      <generator class="identity" />
    </id>
    <property name="RecordingDuration" length="100" type="String">
      <column name="RecordingDuration" />
    </property>
    <property name="RecordingTitle" length="100" type="String">
      <column name="RecordingTitle" />
    </property>
    <property name="RecordingUrl" length="100" type="String">
      <column name="RecordingUrl" />
    </property>
    <property name="RecordingDate" type="DateTime">
      <column name="RecordingDate" />
    </property>
  </class>
</hibernate-mapping>